#include "qcom-msm8974.dtsi"
#include <dt-bindings/clock/qcom,gcc-msm8974.h>

/ {
	model = "Sony Rhine CDB";
	compatible = "sony,msm8974-rhine_cdb", "qcom,msm8974";
	qcom,msm-id = <126 8 0>, <185 8 0>, <186 8 0>;
};

&soc {
	gcc: clock-controller@fc400000 {
		compatible = "qcom,gcc-msm8974";
		#clock-cells = <1>;
		#reset-cells = <1>;
		reg = <0xfc400000 0x4000>;
	};

	serial@f991e000 {
		compatible = "qcom,msm-uartdm-v1.4", "qcom,msm-uartdm";
		reg = <0xf991e000 0x1000>;
		interrupts = <0 108 0x0>;
		clocks = <&gcc GCC_BLSP1_UART2_APPS_CLK>, <&gcc GCC_BLSP1_AHB_CLK>;
		clock-names = "core", "iface";
	};

	spmi: qcom,spmi@fc4c0000 {
		compatible = "qcom,spmi-pmic-arb";
		reg-names = "core", "intr", "cnfg";
		reg = <0xfc4cf000 0x1000>,
		      <0xfc4cb000 0x1000>,
		      <0xfc4ca000 0x1000>;
		interrupts = <0 190 0>;
		#address-cells = <2>;
		#size-cells = <0>;

		interrupt-controller;
		#interrupt-cells = <4>;

		qcom,pm8941@0 {
			compatible = "qcom,pm8941";
			reg = <0x0 0>;

			#address-cells = <1>;
			#size-cells = <0>;

			qcom,rtc@6000 {
				compatible = "qcom,pm8941-rtc";
				reg = <0x6000 0x6100>;
				interrupts = <0x0 97 1 0>;
				interrupt-parent = <&spmi>;
			};

			qcom,revid@100 {
				compatible = "qcom,qpnp-revid";
				reg = <0x100>;
			};

			qcom_vadc: qcom,pm8x41-adc-usr@3100 {
				compatible = "qcom,pm8941-vadc";
				interrupts = <0x0 0x31 0 0>,
					<0x0 0x31 4 0>,
					<0x0 0x31 3 0>;
				interrupt-parent = <&spmi>;
				reg = <0x3100>;
				#io-channel-cells = <1>;
			};

			pm8941_gpios: gpios@c000 {
				compatible = "qcom,qpnp-pin";
				gpio-controller;
				#gpio-cells = <2>;
				#address-cells = <1>;
				#size-cells = <0>;
				label = "pm8941-gpio";

				gpio@c000 {
					reg = <0xc000>;
					qcom,pin-num = <1>;
				};

				gpio@c100 {
					reg = <0xc100>;
					qcom,pin-num = <2>;
					qcom,mode = <0x0>;
					qcom,pull = <0x0>;
					qcom,vin-sel = <0x2>;
					qcom,src-sel = <0x0>;
					qcom,master-en = <0x1>;
				};

				gpio@c200 {
					reg = <0xc200>;
					qcom,pin-num = <3>;
					qcom,mode = <0x0>;
					qcom,pull = <0x0>;
					qcom,vin-sel = <0x2>;
					qcom,src-sel = <0x0>;
					qcom,master-en = <0x1>;
				};

				gpio@c300 {
					reg = <0xc300>;
					qcom,pin-num = <4>;
					qcom,mode = <0x0>;
					qcom,pull = <0x0>;
					qcom,vin-sel = <0x2>;
					qcom,src-sel = <0x0>;
					qcom,master-en = <0x1>;
				};

				gpio@c400 {
					reg = <0xc400>;
					qcom,pin-num = <5>;
					qcom,mode = <0x0>;
					qcom,pull = <0x0>;
					qcom,vin-sel = <0x2>;
					qcom,src-sel = <0x0>;
					qcom,master-en = <0x1>;
				};

				gpio@c500 {
					reg = <0xc500>;
					qcom,pin-num = <6>;
				};

				gpio@c600 {
					reg = <0xc600>;
					qcom,pin-num = <7>;
				};

				gpio@c700 {
					reg = <0xc700>;
					qcom,pin-num = <8>;
				};

				gpio@c800 {
					reg = <0xc800>;
					qcom,pin-num = <9>;
				};

				gpio@c900 {
					reg = <0xc900>;
					qcom,pin-num = <10>;
				};

				gpio@ca00 {
					reg = <0xca00>;
					qcom,pin-num = <11>;
				};

				gpio@cb00 {
					reg = <0xcb00>;
					qcom,pin-num = <12>;
				};

				gpio@cc00 {
					reg = <0xcc00>;
					qcom,pin-num = <13>;
				};

				gpio@cd00 {
					reg = <0xcd00>;
					qcom,pin-num = <14>;
				};

				gpio@ce00 {
					reg = <0xce00>;
					qcom,pin-num = <15>;
				};

				gpio@cf00 {
					reg = <0xcf00>;
					qcom,pin-num = <16>;
				};

				gpio@d000 {
					reg = <0xd000>;
					qcom,pin-num = <17>;
				};

				gpio@d100 {
					reg = <0xd100>;
					qcom,pin-num = <18>;
				};

				gpio@d200 {
					reg = <0xd200>;
					qcom,pin-num = <19>;
				};

				gpio@d300 {
					reg = <0xd300>;
					qcom,pin-num = <20>;
				};

				gpio@d400 {
					reg = <0xd400>;
					qcom,pin-num = <21>;
				};

				gpio@d500 {
					reg = <0xd500>;
					qcom,pin-num = <22>;
				};

				gpio@d600 {
					reg = <0xd600>;
					qcom,pin-num = <23>;
				};

				gpio@d700 {
					reg = <0xd700>;
					qcom,pin-num = <24>;
				};

				gpio@d800 {
					reg = <0xd800>;
					qcom,pin-num = <25>;
				};

				gpio@d900 {
					reg = <0xd900>;
					qcom,pin-num = <26>;
				};

				gpio@da00 {
					reg = <0xda00>;
					qcom,pin-num = <27>;
				};

				gpio@db00 {
					reg = <0xdb00>;
					qcom,pin-num = <28>;
				};

				gpio@dc00 {
					reg = <0xdc00>;
					qcom,pin-num = <29>;
				};

				gpio@dd00 {
					reg = <0xdd00>;
					qcom,pin-num = <30>;
				};

				gpio@de00 {
					reg = <0xde00>;
					qcom,pin-num = <31>;
				};

				gpio@df00 {
					reg = <0xdf00>;
					qcom,pin-num = <32>;
				};

				gpio@e000 {
					reg = <0xe000>;
					qcom,pin-num = <33>;
				};

				gpio@e100 {
					reg = <0xe100>;
					qcom,pin-num = <34>;
				};

				gpio@e200 {
					reg = <0xe200>;
					qcom,pin-num = <35>;
				};

				gpio@e300 {
					reg = <0xe300>;
					qcom,pin-num = <36>;
				};
			};
		};

		qcom,pm8941@1 {
			compatible = "qcom,pm8941";
			reg = <0x1 0>;

			#address-cells = <1>;
			#size-cells = <0>;

			qpnp_pwm: pm8xxx-pwm@b040 {
				compatible = "qcom,pm8xxx-pwm";
				reg = <0xb040>;
				#address-cells = <1>;
				#size-cells = <0>;
				#pwm-cells = <2>;
				qcom,lut = /bits/ 16 <
					0x000 0x008 0x010 0x018 0x020 0x028 0x030 0x038
					0x040 0x048 0x050 0x058 0x060 0x068 0x070 0x078
					0x080 0x088 0x090 0x098 0x0a0 0x0a8 0x0b0 0x0b8
					0x0c0 0x0c8 0x0d0 0x0d8 0x0e0 0x0e8 0x0f0 0x0f8
					0x100 0x108 0x110 0x118 0x120 0x128 0x130 0x138
					0x140 0x148 0x150 0x158 0x160 0x168 0x170 0x178
					0x180 0x188 0x190 0x198 0x1a0 0x1a8 0x1b0 0x1b8
					0x1c0 0x1c8 0x1d0 0x1d8 0x1e0 0x1e8 0x1f0 0x1f8
				>;

				pwm@b500 {
					reg = <0xb500>;
					qcom,channel-id = <4>;
					qcom,ramp-repeat;
					qcom,ramp-pingpong;
					qcom,ramp-duration = <192>;
					qcom,lut-range = <0 64>;
				};

				pwm@b600 {
					reg = <0xb600>;
					qcom,channel-id = <5>;
					qcom,ramp-repeat;
					qcom,ramp-pingpong;
					qcom,ramp-duration = <192>;
					qcom,lut-range = <0 64>;
				};

				pwm@b700 {
					reg = <0xb700>;
					qcom,channel-id = <6>;
					qcom,ramp-repeat;
					qcom,ramp-pingpong;
					qcom,ramp-duration = <192>;
					qcom,lut-range = <0 64>;
				};

			};

			pm8xxx-pwm-led@d000 {
				reg = <0xd000>;
				compatible = "qcom,pm8xxx-pwm-led";
				red-led {
					label = "red";
					qcom,id = "red";
					pwms = <&qpnp_pwm 6 1000>;
				};
				green-led {
					label = "green";
					qcom,id = "green";
					pwms = <&qpnp_pwm 5 1000>;
				};
				blue-led {
					label = "blue";
					qcom,id = "blue";
					pwms = <&qpnp_pwm 4 1000>;
				};
			};

			pm8xxx-wled@d800 {
				compatible = "qcom,pm8941-wled";
				reg = <0xd800>;
				linux,name = "backlight";
				qcom,cs-out;
				qcom,current-limit = <20>;
				qcom,current-boost-limit = <805>;
				qcom,switching-freq = <1600>;
				qcom,ovp = <29>;
				qcom,num-strings = <2>;
			};
 		};
	};

	iio-thermal {
		compatible = "iio-thermal";
		io-channels = <&qcom_vadc 8>;
		iio,conversion-method = "interpolation";
		iio,conversion-values = <
			  30000 398200
			  34000 393200
			  40000 388200
			  46000 383200
			  53000 378200
			  62000 373200
			  73000 368200
			  85000 363200
			 100000 358200
			 118000 353200
			 139000 348200
			 165000 343200
			 195000 338200
			 232000 333200
			 275000 328200
			 326000 323200
			 385000 318200
			 454000 313200
			 533000 308200
			 622000 303200
			 720000 298200
			 825000 293200
			 936000 288200
			1048000 283200
			1159000 278200
			1266000 273200
			1363000 268200
			1451000 263200
			1526000 258200
			1589000 253200
			1641000 248200
			1682000 243200
			1714000 238200
			1738000 233200
		>;

	};

	sfpb_spinlock: qcom,ipc-spinlock@fd484000 {
		compatible = "qcom,ipc-spinlock-sfpb";
		reg = <0xfd484000 0x400>;
		qcom,num-locks = <8>;
	};

	ldrex_spinlock: qcom,ipc-spinlock@fa00000 {
		compatible = "qcom,ipc-spinlock-ldrex";
		reg = <0xfa00000 0x200000>;
		status = "disable";
	};

	msmgpio: pinctrl@fd510000 {
		compatible = "qcom,msm8974-pinctrl";
		reg = <0xfd510000 0x4000>;

		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupts = <0 208 0>;
		qcom,direct-connect-irqs = <8>;

		pinctrl-names = "default";
		pinctrl-0 = <&state_default &uart2_default>;

		state_default: default_active {
			uim_detect {
				pins = "gpio100";

				drive-strength = <2>;
				bias-disable;
			};

			debug_gpio0 {
				pins = "gpio55";
				bias-pull-up;
			};

			debug_gpio1 {
				pins = "gpio56";
				bias-pull-up;
			};

			sdc1_clk_mux: sdc1_clk {
				pins = "sdc1_clk";
				drive-strength = <10>;
				bias-disable;
			};

			sdc1_cmd_mux: sdc1_cmd {
				pins = "sdc1_cmd";
				drive-strength = <10>;
				bias-pull-up;
			};

			sdc1_data_mux: sdc1_data {
				pins = "sdc1_data";
				drive-strength = <16>;
				bias-pull-up;
			};

			i2c2 {
				pins = "gpio6", "gpio7";
				function = "blsp_i2c2";

				drive-strength = <2>;
				bias-disable;
			};

			i2c6 {
				pins = "gpio29", "gpio30";
				function = "blsp_i2c6";

				drive-strength = <2>;
				bias-disable;
			};

		};

		uart2 {
			uart2_default: uart2_default {
				mux {
					pins = "gpio4", "gpio5";
					function = "blsp_uart2";
				};

				tx {
					pins = "gpio4";
					drive-strength = <4>;
					bias-disable;
				};

				rx {
					pins = "gpio5";
					drive-strength = <2>;
					bias-pull-up;
				};
			};
		};


		clearpad_active_state: clearpad_active {
			ts_int {
				pins = "gpio61";

				drive-strength = <2>;
				bias-disable;
			};
		};

		max1187x {
			maxim_default_state: maxim_default {
				ts_int {
					pins = "gpio61";
					drive-strength = <2>;
					bias-disable;
				};

				pwr_en {
					pins = "gpio85";
					drive-strength = <2>;
					bias-disable;
				};
			};
		};
	};

	qcom,smem@fa00000 {
		compatible = "qcom,smem";
		reg = <0xfa00000 0x200000>,
			<0xf9011000 0x1000>,
			<0xfc428000 0x4000>;
		reg-names = "smem", "irq-reg-base", "aux-mem1";
		reserve-memory;

		qcom,smd-modem {
			compatible = "qcom,smd";
			qcom,smd-edge = <0>;
			qcom,smd-irq-offset = <0x8>;
			qcom,smd-irq-bitmask = <0x1000>;
			qcom,pil-string = "modem";
			interrupts = <0 25 1>;
		};

		qcom,smsm-modem {
			compatible = "qcom,smsm";
			qcom,smsm-edge = <0>;
			qcom,smsm-irq-offset = <0x8>;
			qcom,smsm-irq-bitmask = <0x2000>;
			interrupts = <0 26 1>;
		};

		qcom,smd-adsp {
			compatible = "qcom,smd";
			qcom,smd-edge = <1>;
			qcom,smd-irq-offset = <0x8>;
			qcom,smd-irq-bitmask = <0x100>;
			qcom,pil-string = "adsp";
			interrupts = <0 156 1>;
		};

		qcom,smsm-adsp {
			compatible = "qcom,smsm";
			qcom,smsm-edge = <1>;
			qcom,smsm-irq-offset = <0x8>;
			qcom,smsm-irq-bitmask = <0x200>;
			interrupts = <0 157 1>;
		};

		qcom,smd-wcnss {
			compatible = "qcom,smd";
			qcom,smd-edge = <6>;
			qcom,smd-irq-offset = <0x8>;
			qcom,smd-irq-bitmask = <0x20000>;
			qcom,pil-string = "wcnss";
			interrupts = <0 142 1>;
		};

		qcom,smsm-wcnss {
			compatible = "qcom,smsm";
			qcom,smsm-edge = <6>;
			qcom,smsm-irq-offset = <0x8>;
			qcom,smsm-irq-bitmask = <0x80000>;
			interrupts = <0 144 1>;
		};

		qcom,smd-rpm {
			compatible = "qcom,smd";
			qcom,smd-edge = <15>;
			qcom,smd-irq-offset = <0x8>;
			qcom,smd-irq-bitmask = <0x1>;
			interrupts = <0 168 1>;
			qcom,irq-no-suspend;
		};
	};

	qcom,mpm@fc4281d0 {
		compatible = "qcom,mpm-v2";
		reg = <0xfc4281d0 0x1000>, /* MSM_RPM_MPM_BASE 4K */
		    <0xf9011008 0x4>;   /* MSM_APCS_GCC_BASE 4K */
		reg-names = "vmpm", "ipc";
		interrupts = <0 171 1>;

		qcom,ipc-bit-offset = <1>;

		qcom,gic-parent = <&intc>;
		qcom,gic-map = <2 216>, /* tsens_upper_lower_int */
			<47 165>, /* usb30_hs_phy_irq */
			<50 172>, /* usb1_hs_async_wakeup_irq */
			<53 104>, /* mdss_irq */
			<62 222>, /* ee0_krait_hlos_spmi_periph_irq */
			<0xff 56>,  /* modem_watchdog */
			<0xff 57>,  /* mss_to_apps_irq(0) */
			<0xff 58>,  /* mss_to_apps_irq(1) */
			<0xff 59>,  /* mss_to_apps_irq(2) */
			<0xff 60>,  /* mss_to_apps_irq(3) */
			<0xff 70>,  /* iommu_pmon_nonsecure_irq */
			<0xff 97>,  /* iommu_nonsecure_irq */
